.content {
  padding: 20px;
  background-color: #ffffff;
  height: 100rpx;
}

.content>view {
  text-align: center;
}

.content_ul {
  padding: 20rpx;
  border: 1px solid #eeeeee;
  background-color: #ffffff;
}

.content_li {
  padding: 20rpx;
  border: 1px solid #eeeeee;
  background-color: #ffffff;
  font-size: 24rpx;
}

.images {
  width: 200rpx;
  height: 200rpx;
  border-radius: 200rpx;
  margin: 20rpx auto;
}